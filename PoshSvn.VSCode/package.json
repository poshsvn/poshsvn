{
  "name": "poshsvn",
  "displayName": "PoshSvn",
  "description": "Apache Subversion client for PowerShell, integrated to vscode.",
  "version": "@Version@",
  "engines": {
    "vscode": "^1.53.0"
  },
  "homepage": "https://www.poshsvn.com",
  "publisher": "rinrab",
  "icon": "icon.png",
  "author": "Timofei Zhakov",
  "keywords": [
    "Subversion",
    "Svn",
    "PowerShell"
  ],
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rinrab/poshsvn"
  },
  "activationEvents": [],
  "main": "extension.js",
  "contributes": {
    "commands": [
      {
        "command": "PoshSvn.openTerminal",
        "title": "Open PoshSvn terminal"
      }
    ],
    "terminal": {
      "profiles": [
        {
          "title": "PoshSvn terminal",
          "id": "PoshSvn.terminalProfile"
        }
      ]
    },
    "keybindings": [
      {
        "command": "PoshSvn.openTerminal",
        "key": "alt+U"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.53.0",
    "typescript": "^5.3.3"
  }
}
