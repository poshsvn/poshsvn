{
  "name": "poshsvn",
  "displayName": "",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rinrab/poshsvn"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "PoshSvn.open.terminal",
        "title": "Open PoshSvn terminal"
      }
    ],
    "terminal": {
      "profiles": [
        {
          "title": "PoshSvn terminal",
          "id": "PoshSvn.terminal.profile"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm install && tsc -p ./ && powershell.exe ..\\Scripts\\Build-PoshSvn.ps1 && copy ..\\bin\\poshsvn .\\out",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/node": "18.x",
    "typescript": "^5.3.3"
  }
}
